    "ecommerce",
    "vuejs",
    "theme",
    "responsive"
  ],
  "license": "MIT",
  "dependencies": {
    "@mdi/font": "^3.9.96",
    "@vue/cli": "^3.10.0",
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "buefy": "^0.8.2",
    "bulma": "^0.7.2",
    "bulma-helpers": "^0.3.8",
    "eslint-plugin-html": "^6.0.0",
    "express": "^4.16.4",
    "moment": "^2.24.0",
    "vue": "^2.5.21",
    "vue-i18n": "^8.12.0",
    "vue-router": "^3.0.2",
    "vue-social-sharing": "^2.4.5",
    "vuex": "^3.0.1"
